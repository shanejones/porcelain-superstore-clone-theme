<!doctype html>
<html class="no-js supports-no-cookies" lang="{{ shop.locale }}">

  
<head>
  
    <style>
    
        html {
        padding-bottom: 0px !important;
    }
    </style>

    <link rel="preload" href="{{ 'bb-global.min.js' | asset_url  }}" as="script" />

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.color_accent }}">
   {%- comment -%} <link rel="preconnect" rel="canonical" href="{{ canonical_url }}">{%- endcomment -%}

    {% render 'yoast-seo' %}
    {% include 'gtm-script' %}

    {% render 'cls_fixings' %}
    {% if request.host == settings.testing_site_url %}
      {{ 'theme.dev.css' | asset_url | stylesheet_tag }}
    {% else %}
      {{ 'theme.min.css' | asset_url | stylesheet_tag }}
    {% endif %}

    {% if template == 'collection' %}
    <link rel="stylesheet" data-href="{{ 'cloudsearch.scss.css' | asset_url }}" />
    {% endif %}

    {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
    {%- endif -%}

    {%- capture seo_title -%}
    {{ page_title }}
    {%- if current_tags -%}
    {%- assign meta_tags = current_tags | join: ', ' -%} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {%- endif -%}
    {%- if current_page != 1 -%}
    &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {%- endif -%}
    {%- assign escaped_page_title = page_title | escape -%}
    {%- unless escaped_page_title contains shop.name -%}
    &ndash; {{ shop.name }}
    {%- endunless -%}
    {%- endcapture -%}
    {%- comment -%}Auto-disabled. Yoast plugin is taking care of SEO output.{%- endcomment -%}
    {%- if disabled_by_yoast_seo -%}
    <title>{{ seo_title | strip }}</title>
    {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
    {%- endif -%}

    {%- if page_description -%}
    {%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
    {%- if disabled_by_yoast_seo -%}
    <meta name="description" content="{{ page_description | escape }}">
    {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
    {%- endif -%}
    {%- endif -%}

    {% include 'social-meta-tags' %}
    {% comment %} {% include 'style-tags', layout: 'theme' %} {% endcomment %}
    {% include 'css-variables' %}

    {% render 'css-new-fixing' %}


    <script>
      window.dataLayer = window.dataLayer || [];
      dataLayer.push({
        ecommerce: {
          currencyCode: '{{ shop.currency }}',
        }
      });	

      {% if customer %}
      window.dataLayer.push({
        event: 'dl_user_data',
        cart_total: '{{ cart.total_price | divided_by: 100.00 }}',
        user_properties: {
        visitor_type: 'logged_in',
        customer_id: '{{ customer.id }}',
        customer_email: '{{ customer.email }}',
        customer_order_count: '{{ customer.orders_count }}',
        customer_total_spent: {{ customer.total_spent | divided_by: 100.00 }},
        customer_tags: '{{ customer.tags }}',
      }
                            });
      {% else %}
      window.dataLayer.push({
        event: 'dl_user_data',
        cart_total: {{ cart.total_price | divided_by: 100.00 }},
        user_properties: {
        visitor_type: 'guest',
      }
                            });
      {% endif %}
    </script>
    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', '');
      if (navigator.cookieEnabled) document.documentElement.classList.remove('supports-no-cookies');

      window.theme = {
        strings: {
          addToCart: {{ 'products.product.add_to_cart' | t | json }},
          soldOut: {{ 'products.product.sold_out' | t | json }},
          fromTextHtml: {{ 'products.product.from_text_html' | t: price: '[[price]]' | json }},
          unavailable: {{ 'products.product.unavailable' | t | json }}
        },
        moneyFormat: {{ shop.money_format | json }},
        moneyWithCurrencyFormat: {{ shop.money_with_currency_format | json }},
        turbolinksEnabled: {{ settings.turbolinks_enabled }}
      };
    </script>

    {% if settings.turbolinks_enabled %}
    <script src="https://unpkg.com/turbolinks@5.2.0/dist/turbolinks.js" defer></script>
    {% endif %}

    <script type="noscript-s" data-src="https://unpkg.com/jquery@3.6.0/dist/jquery.min.js" ></script>

