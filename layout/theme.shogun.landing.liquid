<script>

      document.querySelector('.hamburger-icon').addEventListener('click', function(){

        if(this.dataset.clickToggle.includes('mobile-nav-open')){
          document.querySelector('body').classList.add('mobile-nav-open')
        }

      });

      document.querySelector('.nav-close-btn').addEventListener('click', function(){

        document.querySelector('body').classList.remove('mobile-nav-open');

      });

      document.querySelectorAll("[data-nav-item='true']").forEach((item)=>{

        item.addEventListener('click', function(){


          if(event.target.innerHTML != "Menu"){

            event.currentTarget.classList.add('menu-open');
          }

        });


      });
      document.querySelectorAll("[data-nav-btn-back]").forEach((item)=>{

        item.addEventListener('click', function(){


          event.target.closest('.menu-open').classList.remove('menu-open');



        });


      });

    </script>
