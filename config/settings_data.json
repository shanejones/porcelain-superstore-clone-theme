          "row1_col1": "<p><strong>By Email<br\/><\/strong><a href=\"mailto:help@porcelainsuperstore.co.uk\" title=\"help@porcelainsuperstore.co.uk\">help@porcelainsuperstore.co.uk<\/a><\/p>",
          "row1_col2": "",
